@model IEnumerable<Garage2.Models.ReceiptViewModel>

@{
    ViewBag.Title = "Receipt";
}

<h2>Kvitto</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ParkTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ParkAt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ParkOut)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cost)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ParkTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParkAt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParkOut)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cost)
        </td>

    </tr>
}

</table>
